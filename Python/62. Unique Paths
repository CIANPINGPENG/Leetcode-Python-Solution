class Solution:
    def uniquePaths(self, m: int, n: int) -> int:
        
        if n-1==0 or m-1==0:
            return 1
        else:
            right=n-1
            down=m-1
            nom=right+down
            nom1=1
            denom=1
            for i in range(nom+1):
                if i != 0:
                    nom1*=i
            for j in range(right+1):
                if j != 0:
                    denom*=j
            for k in range(down+1):
                if k != 0:
                    denom*=k
        return nom1//denom
